class Solution(object):
    def largestNumber(self, nums):
        ch=''
        while len(nums)>0:
            I_Max=0
            for i in range(1,len(nums)):
                if int(str(nums[I_Max])+str(nums[i]))<int(str(nums[i])+str(nums[I_Max])):
                    I_Max=i
            ch+=str(nums[I_Max])
            nums=nums[:I_Max]+nums[I_Max+1:]
        return str(int(ch))
