def searchMatrix(matrix, target):
    if not matrix: return False
    r, c = len(matrix), len(matrix[0])
    l, h = 0, r*c - 1
    while l <= h:
        m = (l + h) // 2
        if matrix[m//c][m%c] == target:
            return True
        elif matrix[m//c][m%c] < target:
            l = m + 1
        else:
            h = m - 1
    return False
